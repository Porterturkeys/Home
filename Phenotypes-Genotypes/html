<button id="returnToTop" style="position: fixed; bottom: 20px; right: 20px; padding: 10px 20px; background-color: #1E82E6; color: white; border: none; border-radius: 5px; cursor: pointer; display: none;">Return to Top</button>

<script>
// Show the button when the user scrolls down 100px from the top of the document
window.onscroll = function() {scrollFunction()};

function scrollFunction() {
    const returnToTopButton = document.getElementById("returnToTop");
    if (document.body.scrollTop > 100 || document.documentElement.scrollTop > 100) {
        returnToTopButton.style.display = "block";
    } else {
        returnToTopButton.style.display = "none";
    }
}

// Scroll smoothly back to the top when the button is clicked
document.getElementById("returnToTop").addEventListener("click", function() {
    window.scrollTo({ top: 0, behavior: 'smooth' });
});
</script>



<script>
document.addEventListener('DOMContentLoaded', function() {
    const baseUrl = 'https://porters-rare-heritage-turkeys.neocities.org/Pictures/';

    document.querySelectorAll('.turkey-image').forEach(function(el) {
        const varietyName = el.getAttribute('data-variety-name').replace(/\s/g, '');
        const genderPrefix = el.classList.contains('male') ? 'M' : el.classList.contains('female') ? 'F' : 'P'; // P for poults
        const imageUrl = baseUrl + genderPrefix + varietyName + '.jpg';
        const genotype = el.getAttribute('data-genotype');

        // Create an image and attempt to load it
        const imgElement = new Image();
        imgElement.onload = function() {
            el.appendChild(imgElement); // Image loads successfully
            appendText(el, genotype, true); // Append genotype next to image
        };
        imgElement.onerror = function() {
            appendText(el, 'Image to come ', false); // Show placeholder text
            appendText(el, genotype, true); // Always display genotype
        };
        imgElement.src = imageUrl;
        imgElement.alt = 'Turkey Image';
        imgElement.style.cursor = 'pointer';
        imgElement.onclick = function() { window.open(imageUrl, '_blank'); };
    });

    function appendText(element, text, isGenotype) {
        // Create a span for text
        const textSpan = document.createElement('span');
        textSpan.textContent = text;
        if (isGenotype) {
            textSpan.classList.add('genotype'); // Apply genotype styling if it is a genotype
        }
        element.appendChild(textSpan);
    }
});

document.addEventListener('DOMContentLoaded', function() {
    // Existing code for loading images...

    // Populate the dropdown with varieties
    const varietySet = new Set();
    document.querySelectorAll('.variety-name').forEach(function(el) {
        varietySet.add(el.textContent);
    });

    const varietySelector = document.getElementById('varietySelector');
    varietySet.forEach(variety => {
        const option = document.createElement('option');
        option.value = variety;
        option.textContent = variety;
        varietySelector.appendChild(option);
    });

    // Function to filter table rows based on selected variety
    varietySelector.addEventListener('change', function() {
        const selectedVariety = this.value;
        document.querySelectorAll('tr').forEach(function(row) {
            if (row.querySelector('.variety-name')) {
                if (selectedVariety === '' || row.querySelector('.variety-name').textContent === selectedVariety) {
                    row.style.display = ''; // Show row
                } else {
                    row.style.display = 'none'; // Hide row
                }
            }
        });
    });
});

</script>

    <!-- Footer -->
<footer style="text-align: center; padding: 20px; background-color: transparent; color: black; font-weight: bold;">
    &copy; 2024 Porter's Rare Heritage Turkeys
</footer>

<!-- JavaScript to toggle the menu on mobile -->
<script>
    document.querySelector('.menu-toggle').addEventListener('click', function() {
        document.querySelector('.navbar ul').classList.toggle('active');
    });
</script>

</body>
</html>
